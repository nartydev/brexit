<%- include('partiels/head'); %>
<body>
    <h2><%= countryName %></h2>
    <div class="number-question"></div>
    <%- include('timeline'); %>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.2.0/socket.io.js"></script>
    <script>
    const socket = io.connect();
    let idSocket

    socket.on('questionValue', _data => {
        console.log(_data)
        // define id
        if(_data.id == 1) {
            idSocket = _data.idSocket;
        } 
        // question next
        if(idSocket == _data.idSocket && _data.id < 11) {
            document.querySelector('.number-question').innerHTML = _data.id
            console.log('bon')
        }
        // success
        if(_data.success) {
            console.log('ok finish')
        }
    })
    </script>
</body>
</html>
  